{
  "name": "gbajs3-workers-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create gbajs3-db",
    "db:migrate": "wrangler d1 execute gbajs3-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute gbajs3-db --local --file=./schema.sql",
    "r2:create": "wrangler r2 bucket create gbajs3-storage"
  },
  "dependencies": {
    "hono": "^4.3.0",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.0"
  }
}
